  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FileSync\FileSync.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Page Update="Controls\Radar\RadarChart.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Controls\Radar\RadarMeter.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
